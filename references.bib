	date = {2012-04},
	note = {Conference Name: {IEEE} Transactions on Geoscience and Remote Sensing},
	keywords = {Atmospheric measurements, Buildings, Data mining, Digital height map, image sequence analysis, morphology, multiangle reflectance, Satellites, Spatial resolution, spectral analysis, Surface topography, texture, urban classification, Vegetation mapping, very high spatial resolution},
	file = {IEEE Xplore Abstract Record:/Users/gavinrolls/Zotero/storage/MKQYL5EA/6032741.html:text/html;IEEE Xplore Full Text PDF:/Users/gavinrolls/Zotero/storage/2LQIRRRT/Longbotham et al. - 2012 - Very High Resolution Multiangle Urban Classificati.pdf:application/pdf},
