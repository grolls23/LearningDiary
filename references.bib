	note = {Publisher: American Association for the Advancement of Science},
	pages = {850--853},
	file = {Full Text PDF:/Users/gavinrolls/Zotero/storage/RCV2HRHQ/Hansen et al. - 2013 - High-Resolution Global Maps of 21st-Century Forest.pdf:application/pdf},
